<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RangeBar">
        <attr name="tickStart" format="float" />
        <attr name="tickEnd" format="float" />
        <attr name="tickInterval" format="float" />
        <attr name="tickHeight" format="dimension" />
        <attr name="tickColor" format="color" />
        <attr name="barWeight" format="dimension" />
        <attr name="rangeBarColor" format="reference|color" />
        <attr name="textColor" format="reference|color" />
        <attr name="pinColor" format="reference|color" />
        <attr name="selectorSize" format="dimension" />
        <attr name="pinPadding" format="dimension" />
        <attr name="pinMinFont" format="dimension" />
        <attr name="pinMaxFont" format="dimension" />
        <attr name="rangeBarPaddingBottom" format="dimension" />
        <attr name="selectorColor" format="reference|color" />
        <attr name="rangeBar" format="boolean" />
        <attr name="temporaryPins" format="boolean" />
        <attr name="connectingLineWeight" format="dimension" />
        <attr name="connectingLineColor" format="reference|color" />
        <attr name="pinRadius" format="dimension" />

    </declare-styleable>

    <declare-styleable name="MonthsBar">

        <attr name="textViewTextSize" format="dimension" />
        <attr name="textViewTextColor" format="reference|color" />

    </declare-styleable>


    <declare-styleable name="ProgressToolbar">

        <attr name="duration" format="float"/>
        <attr name="delay" format="float"/>
        <attr name="progressBarWidth" format="integer"/>
        <attr name="progressBarSpotCount" format="integer"/>
        <attr name="progressBarSpotColor" format="reference|color" />
        <attr name="backgroundColor" format="reference|color" />
        <attr name="progressBar" format="boolean" />

        <attr name="toolBarHeight" format="integer"/>

    </declare-styleable>

</resources>